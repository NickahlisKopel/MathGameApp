{
  "expo": {
    "name": "MathGameApp",
    "slug": "MathGameApp",
    "version": "1.15.0",
    "orientation": "portrait",
    "icon": "./assets/MathGameAppLogoRedesign.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/MathGameAppLogoRedesign.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nickkopel.MathGameApp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "notification": {
      "icon": "./assets/MathGameAppLogoRedesign.png",
      "color": "#4CAF50",
      "androidMode": "default",
      "androidCollapsedTitle": "{{unread_count}} new notifications"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/MathGameAppLogoRedesign.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.nickkopel.MathGameApp"
    },
    "web": {
      "favicon": "./assets/MathGameAppLogoRedesign.png"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./assets/MathGameAppLogoRedesign.png",
          "color": "#4CAF50",
          "sounds": []
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "9f0c084e-8d4f-4f06-8320-ad7270aa2e93"
      },
      "SERVER_URL": "https://mathgameapp.onrender.com"
    }
  }
}
